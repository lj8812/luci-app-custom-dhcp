config global 'global'
    option enable '1'
    option leasetime '12h'

config client 'iphone13'
    option mac 'B2:54:B6:96:E4:8C'
	option hostname 'iphone13'
    option ip '192.168.0.168'
    option gateway '192.168.0.11'
    option dns '192.168.0.11'

